	<xsd:element name="TradeConditionCode" type="clmFECTradeCondition20:TradeConditionCodeContentType"/>
	<!-- =================================================================== -->
	<!-- ===== Type Definitions                                        ===== -->
	<!-- =================================================================== -->
	<!-- ===== Type Definition: Trade Condition Code Content Type  ===== -->
	<!-- =================================================================== -->
	<xsd:simpleType name="TradeConditionCodeContentType">
		<xsd:restriction base="xsd:token">
			<xsd:minLength value="3"/>
			<xsd:maxLength value="3"/>
			<xsd:enumeration value="300">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<ccts:Name>TradePeriodMinimumDeliveryNumber</ccts:Name>
						<ccts:Description>Minimum of delivery orders to be placed based on aggregate supply.</ccts:Description>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="301">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<ccts:Name>TradeSubjectToAgreedConditions</ccts:Name>
						<ccts:Description>There are agreed conditions between buyer and seller.</ccts:Description>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="302">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<ccts:Name>MarketPlaceSupportsOrderAggregation</ccts:Name>
						<ccts:Description>Marketplace supports the aggregation of multiple orders placed on the same supply item.</ccts:Description>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="303">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<ccts:Name>MarketPlaceSupportsOrderCancelation</ccts:Name>
						<ccts:Description>Marketplace supports the cancelation of orders within a specified timeframe</ccts:Description>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="304">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<ccts:Name>MarketPlaceSupportsSpotOrder</ccts:Name>
						<ccts:Description>Marketplace supports the placing of spot orders where the remaining stock is ordered of the ordered item.</ccts:Description>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="305">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<ccts:Name>TradeMinimum</ccts:Name>
						<ccts:Description>Marketplace only accepts orders from customers with a specified minimum turnover.</ccts:Description>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="306">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<ccts:Name>FutureUse</ccts:Name>
						<ccts:Description>This condition is reserved for future use, refer to the marketplace implementation guide</ccts:Description>
					</xsd:documentation>
				</xsd:annotation>
